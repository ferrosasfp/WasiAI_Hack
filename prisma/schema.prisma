// Prisma schema for marketplace AI keys

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ModelKey {
  id        Int      @id @default(autoincrement())
  modelId   Int      @unique
  slug      String?  @db.VarChar(120)
  keyB64    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([slug])
}
