{
  "name": "marketplaceai-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:3002": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "bash -lc 'set -e; chmod -R u+w .next .turbo 2>/dev/null || true; chflags -R nouchg .next .turbo 2>/dev/null || true; find .next -type f -delete 2>/dev/null || true; find .next -type d -empty -delete 2>/dev/null || true; rm -rf .next .turbo'",
    "doctor:port": "bash -lc 'lsof -ti :3002 | xargs -r kill'",
    "rebuild:dev": "npm run clean && npm ci && npm run dev:3002",
    "contracts:build": "bash -lc 'cd contracts/sui && sui move build'",
    "contracts:publish:testnet": "bash -lc 'cd contracts/sui && sui client switch --env testnet && sui client publish --gas-budget 100000000 --json > publish.out.json'",
    "contracts:env:sync": "node scripts/sync-env-from-publish.js",
    "env:use:old": "node scripts/env-switch.js old",
    "env:use:new": "node scripts/env-switch.js new",
    "evm:compile": "bash -lc 'cd contracts/evm && npx hardhat compile'",
    "evm:deploy:base": "bash -lc 'cd contracts/evm && npx hardhat run scripts/deploy.js --network base'",
    "evm:deploy:avax": "bash -lc 'cd contracts/evm && npx hardhat run scripts/deploy.js --network avax'",
    "evm:env:sync": "node scripts/sync-evm-env.js",
    "evm:use:base": "node scripts/evm-env-switch.js base",
    "evm:use:avax": "node scripts/evm-env-switch.js avax"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mysten/dapp-kit": "^0.19.6",
    "@mysten/sui": "^1.43.1",
    "@mysten/sui.js": "^0.54.1",
    "@mysten/wallet-kit": "^0.8.6",
    "@mysten/wallet-standard": "^0.11.6",
    "@prisma/client": "^6.18.0",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.5",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "ethers": "^6.15.0",
    "next": "14.2.33",
    "next-intl": "^4.4.0",
    "process": "^0.11.10",
    "prom-client": "^15.1.3",
    "react": "^18",
    "react-dom": "^18",
    "viem": "^2.38.6",
    "wagmi": "^2.19.2",
    "zod": "^3.25.76",
    "zustand": "^4.5.7"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^20.19.23",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitest/ui": "^4.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.33",
    "eslint-config-prettier": "^9.1.2",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "typescript": "^5",
    "vitest": "^4.0.3"
  }
}
