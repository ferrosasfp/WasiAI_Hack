{
  "name": "marketplaceai-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:3002": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "bash -lc 'set -e; chmod -R u+w .next .turbo 2>/dev/null || true; chflags -R nouchg .next .turbo 2>/dev/null || true; find .next -type f -delete 2>/dev/null || true; find .next -type d -empty -delete 2>/dev/null || true; rm -rf .next .turbo'",
    "doctor:port": "bash -lc 'lsof -ti :3002 | xargs -r kill'",
    "rebuild:dev": "npm run clean && npm ci && npm run dev:3002",
    "indexer": "tsx scripts/run-indexer.ts",
    "cache-metadata": "tsx scripts/cache-all-metadata.ts",
    "refresh-metadata": "tsx scripts/force-refresh-metadata.ts"
  },
  "dependencies": {
