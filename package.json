{
  "name": "marketplaceai-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:3002": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "bash -lc 'set -e; chmod -R u+w .next .turbo 2>/dev/null || true; chflags -R nouchg .next .turbo 2>/dev/null || true; find .next -type f -delete 2>/dev/null || true; find .next -type d -empty -delete 2>/dev/null || true; rm -rf .next .turbo'",
    "doctor:port": "bash -lc 'lsof -ti :3002 | xargs -r kill'",
    "rebuild:dev": "npm run clean && npm ci && npm run dev:3002"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mysten/dapp-kit": "^0.19.6",
    "@mysten/sui": "^1.43.1",
    "@mysten/sui.js": "^0.54.1",
    "@mysten/wallet-standard": "^0.11.6",
    "@prisma/client": "^6.18.0",
    "@tanstack/react-query": "^5.90.5",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "next": "14.2.33",
    "process": "^0.11.10",
    "prom-client": "^15.1.3",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.25.76",
    "zustand": "^4.5.7"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^20.19.23",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitest/ui": "^4.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.33",
    "eslint-config-prettier": "^9.1.2",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "typescript": "^5",
    "vitest": "^4.0.3"
  }
}
